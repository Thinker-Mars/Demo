<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cone.sunny.dao.UserLoginDao">
     <!-- 根据登陆数据查询用户信息  -->
     <select id="SearchWorkerInfoByLogin"  resultType="map">
         select id,name,role from worker where username=#{username} and password=#{password}
     </select>

     
     
     
</mapper>